{
    "name": "vscode-twiki",
    "displayName": "TWiki/Foswiki",
    "description": "TWiki and Foswiki Language Support",
    "version": "1.0.0",
    "publisher": "darryllawson",
    "homepage": "https://github.com/darryllawson/vscode-twiki/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/darryllawson/vscode-twiki.git"
    },
    "bugs": {
        "url": "https://github.com/darryllawson/vscode-twiki/issues",
        "email": "darryl.lawson@me.com"
    },
    "galleryBanner": {
        "color": "#BDE988",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "twiki",
            "aliases": ["TWiki/Foswiki", "twiki"],
            "extensions": [".twiki", ".foswiki"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "twiki",
            "scopeName": "text.html.twiki",
            "path": "./syntaxes/twiki.tmLanguage"
        }],
        "snippets": [{
            "language": "twiki",
            "path": "./snippets/twiki.json"
        }]
    }
}
